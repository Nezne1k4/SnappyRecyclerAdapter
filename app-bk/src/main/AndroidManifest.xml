<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.silong.snappyrecyclerviewadapter">

  <application android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:name="com.silong.snappyrecycleradapter.App"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name="com.silong.snappyrecycleradapter.MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name="com.silong.snappyrecycleradapter.RecyclerViewActivity" />
    <activity android:name="com.silong.snappyrecycleradapter.ListViewActivity" />
  </application>

</manifest>
